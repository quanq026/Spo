{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "crons": [
    {
      "path": "/keep-alive",
      "schedule": "*/5 * * * *"
    }
  ],
  "env": {
    "SPOTIFY_ACCESS_TOKEN": "@spotify_access_token",
    "SPOTIFY_REFRESH_TOKEN": "@spotify_refresh_token"
  }
}
